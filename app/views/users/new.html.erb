<h1>注册</h1>
<%= form_for @user, :url => users_path do |f| %>
    <%= label_tag "user_email", "邮箱：" %>
    <%= f.text_field :email %>
    <label for="user_name">用户名：</label>
    <%= f.text_field :name %>
    <label for="user_password">密码：</label>
    <%= f.password_field :password %>
    <%= f.submit "注册" %>
<% end %>